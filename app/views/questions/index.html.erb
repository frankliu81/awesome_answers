<h1>All Questions</h1>
<% @questions.each do |q| %>
  <div>
    <%#= link_to q.title, question_path(q)%>
    <%= link_to question_path(q) do %>
      <%= q.title %>
      <% if q.image.present? %>
        <%= image_tag q.image.url(:thumb) %>
      <% end %>
    <% end %>
    <hr>
  </div>
<% end %>
